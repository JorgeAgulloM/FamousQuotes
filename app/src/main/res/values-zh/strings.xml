    <!-- Historical default quotes -->
    <string name="historical_quote_default_01">ZH#想象力比知识更重要</string>
    <string name="historical_quote_default_02">ZH#如果我做出了宝贵的发现，那更多是因为耐心，而不是其他天赋</string>
